
<div class="row article-new-title"><div class="col-md-offset-1 col-md-10">
	<h1>You're editting the article</h1>
</div></div>

<%= form_for @edit_article, :url=>article_path, :method=>:patch do |form| %>

	<%= render :partial => "post_form", :locals =>{:form=>form}%>

	<%= form.submit "Update", :class=>"btn btn-info"%>
	<%= link_to "Back",article_path, :class=>"btn btn-primary"%>
<% end %>
</div></div>

